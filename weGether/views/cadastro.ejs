<%- include('./partials/head'); %>

<%- include('./partials/header'); %>

<div class="cadastro">
    <h1 class="form-titulo">Cadastro</h1>
    <form class="form-horizontal formulario" action="">
        <div class="form-group row">
        <div class="col-sm-12">
            <input placeholder="Nome completo..." type="text" class="form-control" name="nome">
        </div>
        </div>
        <div class="form-group row">
        <div class="col-sm-9">
            <input placeholder="Email..."type="email" class="input-cadastro" name="email">
        </div>   
        </div>

        <div class="form-group row"> 
        <div class="col-sm-9">
            <input placeholder="(99)99999-9999..."type="text" class="input-cadastro" name="telefone">   
        </div>
        </div>
        
        <div class="form-group row">
        <div class="col-sm-9">
            <input placeholder="Nome da Empresa que trabalha..."type="text" class="input-cadastro" name="empresa">
        </div>
        </div>

        <div class="form-group row">
        <div class="col-sm-9">
            <input placeholder="Cargo que exerce..."type="text" class="input-cadastro" name="cargo">
        </div>
        </div>

        <div class="form-group row">    
        <div class="col-sm-9">
            <input placeholder="*******"type="text" class="input-cadastro" name="senha">
        </div>
        </div>  
           
            
        </form>

        <button href="/dados-pessoais" type="submit"><a href="/dados-pessoais">Confirmar</a></button>

</div>


<%- include('./partials/footer'); %>